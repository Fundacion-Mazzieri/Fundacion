@model Fundacion.Models.Login
@{
    ViewData["Title"] = "Ingreso";
    Layout = null;

}
<form asp-action="Ingresar">
    <div class="form-group">
        <input type="text" value="correo" asp-for="Correo" class="btn btn-primary" />
    </div>
    <div class="form-group">
        <input type="text" value="clave" asp-for="Clave" class="btn btn-primary" />
        <span asp-validation-for="Clave" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Ingresar" class="btn btn-primary" />
    </div>
</form>
